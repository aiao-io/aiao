    "lazy-module-demo": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/lazy-module-demo",
      "sourceRoot": "apps/lazy-module-demo/src",
      "prefix": "aiao",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/lazy-module-demo",
            "index": "apps/lazy-module-demo/src/index.html",
            "main": "apps/lazy-module-demo/src/main.ts",
            "polyfills": "apps/lazy-module-demo/src/polyfills.ts",
            "tsConfig": "apps/lazy-module-demo/tsconfig.app.json",
            "aot": false,
            "assets": ["apps/lazy-module-demo/src/favicon.ico", "apps/lazy-module-demo/src/assets"],
            "styles": ["apps/lazy-module-demo/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/lazy-module-demo/src/environments/environment.ts",
                  "with": "apps/lazy-module-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "lazy-module-demo:build",
            "port": 4201
          },
          "configurations": {
            "production": {
              "browserTarget": "lazy-module-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "lazy-module-demo:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/lazy-module-demo/tsconfig.app.json", "apps/lazy-module-demo/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/lazy-module-demo/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/lazy-module-demo/jest.config.js",
            "tsConfig": "apps/lazy-module-demo/tsconfig.spec.json",
            "setupFile": "apps/lazy-module-demo/src/test-setup.ts"
          }
        }
      }
    },
