<!DOCTYPE html>
<html dir="ltr" lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=5.0" />
    <title>Stencil Component Starter</title>

    <!-- test -->
    <link href="/scripts/testing/styles.css" rel="stylesheet" />
    <script src="/scripts/testing/scripts.js"></script>

    <!-- vendors -->
    <% if(vendors.ionic){  %>
    <!-- ionic -->
    <link href="/vendors/ionic/css/ionic.bundle.css" rel="stylesheet" />
    <script type="module" src="/vendors/ionic/dist/ionic/ionic.esm.js"></script>
    <script nomodule src="/vendors/ionic/dist/ionic/ionic.js"></script>
    <% } %>
    <!-- library -->
    <script type="module" src="/dist/<%= name %>/<%= name %>.esm.js"></script>
    <script nomodule src="/dist/<%= name %>/<%= name %>.js"></script>
  </head>
  <body>
   <% if(vendors.ionic){  %><ion-button>ion-button</ion-button><% } %>
   <my-component first="Stencil" last="'Don't call me a framework' JS"></my-component>
  </body>
</html>
