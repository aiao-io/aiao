{
  "projectType": "library",
  "root": "libs/lazy-module",
  "sourceRoot": "libs/lazy-module/src",
  "prefix": "aiao",
  "architect": {
    "build": {
      "builder": "@nrwl/angular:package",
      "options": { "tsConfig": "libs/lazy-module/tsconfig.lib.json", "project": "libs/lazy-module/ng-package.json" },
      "configurations": { "production": { "tsConfig": "libs/lazy-module/tsconfig.lib.prod.json" } }
    },
    "lint": {
      "builder": "@nrwl/linter:eslint",
      "options": { "lintFilePatterns": ["libs/lazy-module/src/**/*.ts", "libs/lazy-module/src/**/*.html"] },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "builder": "@nrwl/jest:jest",
      "options": { "jestConfig": "libs/lazy-module/jest.config.js", "passWithNoTests": true },
      "outputs": ["coverage/libs/lazy-module"]
    }
  },
  "tags": ["angular-lib"]
}
