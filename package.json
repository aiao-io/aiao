{
  "name": "aiao",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --code-coverage",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "build:libs": "yarn check:sylink && lerna run build --scope @aiao/* && yarn format",
    "build:tools": "rimraf dist/tools && tsc -p ./tools/tsconfig.tools.json",
    "check:commit": "node dist/tools/scripts/commit-lint.js",
    "check:lib-build": "node dist/tools/scripts/check-lib-build.js",
    "check:sylink": "node dist/tools/scripts/check-sylink.js",
    "check:workspace": "node dist/tools/scripts/check-workspace --lib",
    "clean": "git clean -dxf",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "postinstall": "yarn build:tools && yarn task:hotfix && yarn check:workspace && yarn postngcc",
    "postngcc": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "preinstall": "rm -rf node_modules/@aiao",
    "prepush": "yarn check:commit && yarn format:check",
    "run:tools": "ts-node --project ./tools/tsconfig.tools.json -r tsconfig-paths/register",
    "task:lodash-optimize": "yarn node dist/tools/scripts/affected-lodash-optimize && yarn format",
    "task:hotfix": "node dist/tools/hotfix/hotfix"
  },
  "private": true,
